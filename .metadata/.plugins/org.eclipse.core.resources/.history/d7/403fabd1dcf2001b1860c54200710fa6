package team_learn;

import java.util.Scanner;

public class Quiz03_funtion {

	Scanner input = new Scanner(System.in);
	int select =0;
	
	
public void humidifier_Power()
{
	select = input.nextInt();
	
	if(select == 1)
	{
		System.out.println("전원 on");
	}
	else if(select == 2 )
	{
		System.out.println("전원 off");
	}
}
}
