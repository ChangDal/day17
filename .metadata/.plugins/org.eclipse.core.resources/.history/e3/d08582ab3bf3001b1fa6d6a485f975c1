package team_learn;

import java.util.ArrayList;
import java.util.Scanner;

public class Quiz06_funtion {
String ID,PW;  
Scanner input = new Scanner(System.in);
ArrayList alist = new ArrayList();
public void ID_PW_login() {
	
	System.out.println("아이디 입력");
	SetID(input.next());
	alist = GetID();
	System.out.println("비밀번호 입력");
}


public void SetID(ArrayList ID) {	this.ID = ID;}

public ArrayList GetID() {return this.ID;}

public void SetPW(ArrayList PW) {this.PW = PW;}

public ArrayList GetPW() {return this.PW;}
	
}
