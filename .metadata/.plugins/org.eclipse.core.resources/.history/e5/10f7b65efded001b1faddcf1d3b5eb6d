package day11;

import java.util.ArrayList;

public class Lotto {
ArrayList<Integer> suffle(ArrayList<Integer> aList){
	
	
	while(aList.size() < 6) {
		int rand = (int) Math.floor((Math.random()*(46)+1));
		
		if(!aList.contains(rand))
		{
			aList.add(rand);
		}
		
		
		
	}
	
	
	return aList;
	
}
}
